{
  //Local rabbit instance/container
  "Publisher.Settings": {
    "Protocol": "amqp",
    "Namespace": "localhost",
    "Policy": "admin",
    "Key": "password",
    "AppName": "test.publisher",
    "Address": "/exchange/test.exchange/",
    "Durable": "1"
  },
  "Receiver.Settings": {
    "Protocol": "amqp",
    "Namespace": "localhost",
    "Policy": "admin",
    "Key": "password",
    "AppName": "test.receiver",
    "Address": "test.queue",
    "Durable": "1"
  },

  //Azure Service Bus
  "xPublisher.Settings": {
    "Protocol": "amqps",
    "Namespace": "test.servicebus.windows.net",
    "Policy": "SendListen",
    "Key": "key",
    "AppName": "test.publisher",
    "Address": "test.",
    "Durable": "1"
  },
  "xReceiver.Settings": {
    "Protocol": "amqps",
    "Namespace": "test.servicebus.windows.net",
    "Policy": "SendListen",
    "Key": "key",
    "AppName": "test.receiver",
    "Address": "test.testevent/Subscriptions/subscription.TestEvent",
    "Durable": "1"
  }
}
